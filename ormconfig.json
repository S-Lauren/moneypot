 {
   "type": "sqlite",
   "database": "database.sqlite",
   "synchronize": true,
   "autoReconnect": true,
   "entities": [
      "src/entity/**/*.ts"
   ],
   "migrations": [
      "src/migration/**/*.ts"
   ],
   "cli": {
      "entitiesDir": "src/entity",
      "migrationsDir": "src/migration"
   }
} 